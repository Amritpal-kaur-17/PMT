<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Iteration_Number_Can_not_be_Changed_PMT</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT(ISNEW()) ,ISCHANGED( Iteration_Number_PMT__c ))</errorConditionFormula>
    <errorDisplayField>Iteration_Number_PMT__c</errorDisplayField>
    <errorMessage>It can not be changed iteration number</errorMessage>
</ValidationRule>
